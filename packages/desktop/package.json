{
	"name": "realesta-desktop",
	"description": "Property Management System",
	"version": "0.0.1",
	"private": true,
	"main": "public/electron.js",
	"homepage": ".",
	"productName": "Realesta",
	"license": "MIT",
	"directories": {
		"lib": "lib",
		"test": "__tests__"
	},
	"files": [ "lib" ],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hytechlab/abacus.git"
	},
	"scripts": {
		"analyze": "cross-env ANALYZE=1 umi build",
		"build": "umi build",
		"deploy": "npm run site && npm run gh-pages",
		"dev": "npm run start:dev",
		"fetch:blocks": "pro fetch-blocks && npm run prettier",
		"gh-pages": "gh-pages -d dist",
		"i18n-remove": "pro i18n-remove --locale=zh-CN --write",
		"postinstall": "umi g tmp",
		"lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
		"lint-staged": "lint-staged",
		"lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
		"lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
		"lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
		"lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
		"lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
		"prettier": "prettier -c --write \"**/*\"",
		"start": "umi dev",
		"start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
		"start:no-mock": "cross-env MOCK=none umi dev",
		"start:no-ui": "cross-env UMI_UI=none umi dev",
		"start:pre": "cross-env REACT_APP_ENV=pre umi dev",
		"start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
		"pretest": "node ./tests/beforeTest",
		"test": "umi test",
		"test:all": "node ./tests/run-tests.js",
		"test:component": "umi test ./src/components",
		"tsc": "tsc",
		"electron-dev": "concurrently \"BROWSER=none yarn dev\" \"wait-on http://localhost:8000 && electron .\"",
		"electron-pack": "electron-builder --linux",
		"preelectron-pack": "yarn build"
	},
	"husky": {
		"hooks": {
			"pre-commit": ""
		}
	},
	"lint-staged": {
		"**/*.less": "stylelint --syntax less",
		"**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
		"**/*.{js,jsx,tsx,ts,less,md,json}": [ "prettier --write" ]
	},
	"browserslist": [ "> 1%", "last 2 versions", "not ie <= 10" ],
	"dependencies": {
		"@ant-design/charts": "^0.9.13",
		"@ant-design/icons": "^4.0.0",
		"@ant-design/pro-layout": "^6.0.0",
		"@ant-design/pro-table": "^2.4.0",
		"@antv/data-set": "^0.11.0",
		"@types/lodash.debounce": "^4.0.6",
		"antd": "^4.4.0",
		"bizcharts": "^3.5.3-beta.0",
		"bizcharts-plugin-slider": "^2.1.1-beta.1",
		"case-anything": "^1.1.1",
		"classnames": "^2.2.6",
		"countries-list": "^2.5.6",
		"data-uri-to-buffer": "^3.0.1",
		"dayjs": "^1.8.36",
		"electron-is-dev": "^1.2.0",
		"electron-log": "^4.2.4",
		"electron-updater": "^4.3.5",
		"faker": "^5.1.0",
		"libphonenumber-js": "^1.7.55",
		"lodash": "^4.17.19",
		"lodash.debounce": "^4.0.8",
		"md5": "^2.2.1",
		"mocker-data-generator": "^2.9.0",
		"moment": "^2.25.3",
		"numeral": "^2.0.6",
		"omit.js": "^1.0.2",
		"path-to-regexp": "2.4.0",
		"pouchdb": "^7.2.1",
		"pouchdb-find": "^7.2.1",
		"qs": "^6.9.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-enterprise-table": "^1.0.7",
		"react-fittext": "^1.0.0",
		"react-helmet-async": "^1.0.4",
		"react-html-table-to-excel": "^2.0.0",
		"react-to-pdf": "^0.0.11",
		"react-to-print": "^2.10.0",
		"recompose": "^0.30.0",
		"redux-form": "^8.3.6",
		"taffy": "^2.6.2",
		"taffydb": "^2.7.3",
		"umi": "^3.1.2",
		"umi-request": "^1.0.8",
		"use-merge-value": "^1.0.1"
	},
	"devDependencies": {
		"@ant-design/pro-cli": "^1.0.18",
		"@types/classnames": "^2.2.7",
		"@types/express": "^4.17.0",
		"@types/history": "^4.7.2",
		"@types/jest": "^26.0.0",
		"@types/lodash": "^4.14.144",
		"@types/qs": "^6.5.3",
		"@types/react": "^16.9.17",
		"@types/react-dom": "^16.8.4",
		"@types/react-helmet": "^5.0.13",
		"@umijs/fabric": "^2.2.0",
		"@umijs/plugin-blocks": "^2.0.5",
		"@umijs/preset-ant-design-pro": "^1.2.0",
		"@umijs/preset-react": "^1.4.8",
		"@umijs/preset-ui": "^2.0.9",
		"carlo": "^0.9.46",
		"chalk": "^4.0.0",
		"concurrently": "^5.2.0",
		"cross-env": "^7.0.0",
		"cross-port-killer": "^1.1.1",
		"detect-installer": "^1.0.1",
		"electron": "^9.1.0",
		"electron-builder": "^22.7.0",
		"electron-devtools-installer": "^2.2.4",
		"electron-is-dev": "^1.1.0",
		"enzyme": "^3.11.0",
		"eslint": "^7.1.0",
		"express": "^4.17.1",
		"gh-pages": "^3.0.0",
		"husky": "^4.0.7",
		"jsdom-global": "^3.0.2",
		"lint-staged": "^10.0.0",
		"mockjs": "^1.0.1-beta3",
		"prettier": "^2.0.1",
		"pro-download": "1.0.1",
		"puppeteer-core": "^4.0.1",
		"stylelint": "^13.0.0",
		"wait-on": "^5.1.0"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"checkFiles": [ "src/**/*.js*", "src/**/*.ts*", "src/**/*.less", "config/**/*.js*", "scripts/**/*.js" ],
	"build": {
		"appId": "com.hytechlab.realesta",
		"productName": "Abacus",
		"copyright": "Copyright Â© 2020 Madrachi Company Limited",
		"win": {
			"icon": "icons/win/icon.ico",
			"target": [
				{
					"target": "nsis",
					"arch": [ "x64" ]
				}
			]
		},
		"nsis": {
			"oneClick": false,
			"installerIcon": "icons/win/icon.ico",
			"uninstallerIcon": "icons/win/icon.ico",
			"allowToChangeInstallationDirectory": true,
			"license": "LICENSE.md",
			"runAfterFinish": true,
			"perMachine": false,
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true,
			"deleteAppDataOnUninstall": true
		},
		"linux": {
			"target": [ "AppImage" ],
			"category": "Busness"
		},
		"appImage": {
			"license": "LICENSE"
		},
		"files": [ "icons/**/*", "node_modules/**/*" ],
		"directories": {
			"buildResources": "assets"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "therickm",
				"repo": "realesta-pms",
				"releaseType": "draft",
				"private": true,
				"token": "6dd663ef72fdcce97c2dbf9aff3dfddd4e558667"
			}
		]
	},
	"author": {
		"name": "Mudali Derick",
		"email": "mudalit@gmail.com.com"
	}
}
